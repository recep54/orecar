<p id="notice"><%= notice %></p>

<h1>Japans</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @japans.each do |japan| %>
      <tr>
        <td><%= link_to 'Show', japan %></td>
        <td><%= link_to 'Edit', edit_japan_path(japan) %></td>
        <td><%= link_to 'Destroy', japan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Japan', new_japan_path %>
